   "source": [
    "gender= {'Female': 0, 'Male': 1}\n",
    "a['Gender'] = a['Gender'].map(gender)\n",
    "features = ['Age', 'Gender']\n",
    "p = a[features]\n",
    "q = a['Income']\n",
    "dtree = DecisionTreeClassifier()\n",
    "dtree.fit(p, q)\n",
    "plt.figure(figsize=(12,8))\n",
    "plot_tree(dtree, feature_names=features,filled=True)\n",
    "plt.show()"
   ]
  }
